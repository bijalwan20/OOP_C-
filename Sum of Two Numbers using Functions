//C++ Program to find Sum of Two Numbers using Functions.

#include <iostream>
using namespace std;

int sum(int a, int b)
{
    int c;
    c = a + b;
    return c;
}

int main()
{
    int num1, num2;
    cout <<"Enter the first number : ";
    cin >> num1;
    cout <<"\nEnter the second number : ";
    cin >> num2;
    
    cout <<"\nThe sum of two numbers is "<<sum(num1, num2)<<endl;
    return 0;
}
